{
	"name": "cakephp/authorization",
	"description": "Authorization abstraction layer plugin for CakePHP",
	"keywords": [
		"auth",
		"authorization",
		"access",
		"cakephp"
	],
	"type": "cakephp-plugin",
	"require": {
		"cakephp/core": "~3.4"
	},
	"require-dev": {
		"cakephp/cakephp": "~3.4",
		"cakephp/cakephp-codesniffer": "^3.0",
		"phpunit/phpunit": "<6.0"
	},
	"suggest": {
		"cakephp/utility": "Provides CakePHP security methods. Required for the JWT adapter and Legacy password hasher."
	},
	"license": "MIT",
	"minimum-stability": "stable",
	"autoload": {
		"psr-4": {
			"Authorization\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AuthTest\\": "tests/test_app/Plugin/AuthTest/src",
			"Authorization\\Test\\": "tests",
			"Authorization\\Test\\App\\": "tests/test_app/App",
			"Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
			"TestApp\\": "tests/test_app",
			"TestPlugin\\": "tests/test_app/Plugin/TestPlugin/src"
		}
	},
	"authors": [
		{
			"name": "Florian KrÃ¤mer",
			"homepage": "http://florian-kraemer.net",
			"role": "Developer"
		}
	]
}
